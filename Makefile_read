PROGRAM := vm-test
OBJS := hash.o vector.o read.o vm.o object.o symbol.o CException.o lexer.o array.o


CC := gcc
#FLAGS := -g -O0
FLAGS := -O3
LDLIBS :=  -lgc -lgmp  -lmpfr -lm  -lblas  -llapacke

$(PROGRAM): $(OBJS)
	$(CC) -o $(PROGRAM) $(FLAGS) $^ $(LDLIBS)

$(OBJS): general.h hash.h vector.h vm.h object.h symbol.h lexparse.h array.h

.SUFFIXES: .c .o
.c.o:
	$(CC) $(FLAGS) -c $<
clean:
	$(RM) *.o
	$(RM) vm-test
